{
	"name": "cra-template",
	"description": "The base template for Create React App.",
	"license": "MIT",
	"version": "0.1.0",
	"files": [
		"template",
		"template.json"
	],
	"scripts": {
		"clean": "node bin/clean.js",
		"prebuild": "npm run clean",
		"build": "node bin/build.js",
		"preprettier": "prettier-package-json --write",
		"preprettier:check": "prettier-package-json -l",
		"prettier": "prettier --write .",
		"prettier:check": "prettier --check ."
	},
	"devDependencies": {
		"fs-extra": "^9.0.1",
		"husky": "^4.2.5",
		"ignore": "^5.1.8",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"prettier-package-json": "^2.1.3",
		"pretty-quick": "^2.0.1"
	},
	"keywords": [
		"airbnb",
		"create-react-app",
		"eslint",
		"json-server",
		"prettier",
		"react",
		"react-router",
		"template"
	],
	"engines": {
		"node": ">=10"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && pretty-quick --staged"
		}
	},
	"lint-staged": {
		"package.json": "prettier-package-json --write"
	}
}
